/* pages/header/index.wxss */
@import '../../css/public.wxss';
.header{
  display: flex;
  flex-direction: row;
  background-color:transparent;
  justify-content: center;
  align-items: center;
  font-size: 1.5rem;
  position: fixed;
  width: 100%;
  z-index: 9;
  top:0;
  left:0;
  font-size: 16px;
  cursor: default;
}
.header .back{
  position: absolute;
  left: 10px;
  /* width: 24px;
  height: 24px; */
  overflow: hidden;
  max-width: 24px;
  max-height: 24px;
}
.header-back{
  width: 50%;
  height: 100%;
  margin-right: 50%;
}
